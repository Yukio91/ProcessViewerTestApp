<Application x:Class="ProcessViewerTestApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:ProcessViewerTestApp.Converters"
             xmlns:local="clr-namespace:ProcessViewerTestApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/LocalizableStrings.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <converters:IconConverter x:Key="IconConverter" />
        </ResourceDictionary>        

    </Application.Resources>
</Application>
